@echo off
setlocal enabledelayedexpansion
set 

:start
if EXIST FilledSpace.txt break>FilledSpace.txt
cls
title Disk Space Filler
call logo.bat
echo.
echo Disk Space Filler
echo =======================
echo Please enter how many bytes you would like to fill:
set /p bytes=^> 
if %bytes% GTR 50000000 call :methods
goto 2

:1
:: Calculate number of lines required, each line is 5000 bytes
set dsf=49980006
set /a lines=%dsf% / 5000

:: Write the calculated number of lines to the file
for /l %%i in (1,1,%lines%) do echo %space%>>FilledSpace.txt

:: Write remaining bytes (if any)
set /a remainder=%dsf% %% 5000
if %remainder% NEQ 0 (
    set "zeros=%space%"
    echo !zeros:~0,%remainder%!>>FilledSpace.txt
)


if NOT EXIST "Disk Space Filler" md "Disk Space Filler"
for /L %%i in (50000000,50000000,%bytes%) do copy FilledSpace.txt "Disk Space Filler\FilledSpace_%%i.txt" >nul
del FilledSpace.txt >nul
if %ERRORLEVEL% EQU 0 echo Success!
if NOT %ERRORLEVEL% EQU 0 echo Error: %ERRORLEVEL%!
pause
goto :eof


:2
:: Calculate number of lines required, each line is 5000 bytes
set /a lines=%bytes% / 5000

:: Write the calculated number of lines to the file
for /l %%i in (1,1,%lines%) do echo %space%>>FilledSpace.txt

:: Write remaining bytes (if any)
set /a remainder=%bytes% %% 5000
if %remainder% NEQ 0 (
    echo !space:~0,%remainder%!>>FilledSpace.txt
)

:methods
echo.
echo Please choose a filling method:
echo 1. [32mSpammer[0m (Faster, more resource-heavy)
echo 2. [31mNullifier[0m (Much slower, less resorce-heavy)
set /p method=^> 
goto %method%

endlocal
pause
exit
